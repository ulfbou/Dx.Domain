<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>

    <AssemblyName>Dx.Domain.Abstractions</AssemblyName>
    <RootNamespace>Dx.Domain</RootNamespace>

    <IsAbstractionsProject>true</IsAbstractionsProject>

    <IsDomainProject>false</IsDomainProject>
    <IsToolingProject>false</IsToolingProject>
  </PropertyGroup>

  <PropertyGroup Label="Dx.Abstractions.Quality">
    <Nullable>enable</Nullable>
    <TreatWarningsAsErrors>true</TreatWarningsAsErrors>
    <GenerateDocumentationFile>true</GenerateDocumentationFile>
  </PropertyGroup>

</Project>
